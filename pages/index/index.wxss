/* 公告 */
.example {
  display: block;
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 20rpx;
  overflow: hidden;
}
@keyframes run {
  0% { margin-left: 100%}
  100% { margin-left: -100%}
}
.marquee_box {
  height: 40rpx;
  position: relative;
  overflow: hidden;
  animation: run 9s linear infinite;
}
.inform_img{
  width: 50rpx;
  height: 50rpx;
  margin-left: 10rpx;
}
.marquee_text {
  white-space: nowrap;
  position: absolute;
  top: 0;
  left: 0;
  height: 40rpx;
  line-height: 40rpx;
  color: #b4282d;
}
.box_shodow{
  box-shadow: 0 0 10rpx #ccc;
}
.border-radius{
  border-radius: 10rpx;
}