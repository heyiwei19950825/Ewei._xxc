<view class="container">
  <view class="profile-info" bindtap="goLogin">
    <image class="avatar" src="{{userInfo.portrait}}"></image>
    <view class="info">
      <text class="name">{{userInfo.nickname}}</text>
      <!-- <text class="level">{{userInfo.username}}</text>   -->
    </view>
    <view class=' integral flex-row'>
            <view class="flex-grow-0 flex-y-center">
                <image class="option-icon" src="/static/img/integral.png" />
            </view>
            <view class="flex-grow-1 flex-y-center">
                积分
                <view class='flex-y-center integral-num'>{{userInfo.integral}}</view>
            </view>
            <navigator class='flex-row' url='/pages/newGoods/newGoods'>
                <view class="flex-grow-1 flex-y-center user-integral-go">
                    去购物抵扣
                </view>
                <view class="flex-grow-0 flex-y-center user-integral-arrow">
                    <image class="option-icon" src="/static/img/user-integral-arrow.png" />
                </view>
            </navigator>

        </view>
  </view>

  <view class="user-menu">
    <view class="item">
      <navigator url="/pages/ucenter/order/order" class="a">
        <text class="icon order"></text>
        <text class="txt">我的订单</text>
      </navigator>
    </view>
    <view class="item">
      <navigator url="/pages/ucenter/collective/collective" class="a">
        <text class="icon gift"></text>
        <text class="txt">我的团购</text>
      </navigator>
    </view>   
    <view class="item">
      <navigator url="/pages/ucenter/coupon/coupon" class="a">
        <text class="icon coupon"></text>
        <text class="txt">优惠券</text>
      </navigator>
    </view>
    <view class="item">
      <navigator url="/pages/ucenter/collect/collect" class="a">
        <text class="icon collect"></text>
        <text class="txt">我的收藏</text>
      </navigator>
    </view>
    <view class="item">
      <navigator url="/pages/ucenter/footprint/footprint" class="a">
        <text class="icon security"></text>
        <text class="txt">我的足迹</text>
      </navigator>
    </view>
    <view class="item">
      <navigator url="../address/address" class="a">
        <text class="icon address"></text>
        <text class="txt">地址管理</text>
      </navigator>
    </view>
    <view class="item">
      <navigator url="/pages/newGoods/newGoods" class="a">
        <text class="icon integral"></text>
        <text class="txt">积分中心</text>
      </navigator>
    </view>
    <view class="item" >
       <navigator url="../member/member" class="a"> 
      <!-- <navigator url="/pages/auth/register/register" class="a"> -->
        <text class="icon vip"></text>
        <text class="txt">会员中心</text>
      </navigator>
    </view>  
  
     <view class="item">
      <navigator url="../enter/enter" class="a">
        <text class="icon shop"></text>
        <text class="txt">商家入驻</text>
      </navigator>
    </view> 
    <!-- <view class="item">
      <navigator url="url" class="a">
        <text class="icon security"></text>
        <text class="txt">账号安全</text>
      </navigator>
    </view>  -->
     <view class="item ke-box"  bindtap='telKefu'>
      <view class="a">
          <text class="icon kefu"></text>
          <text class="txt">联系客服</text>
      </view>
    </view> 
     <view class="item">
       <navigator url="/pages/helper/helper" class="a"> 
      <!-- <navigator url="/pages/auth/login/login" class="a"> -->
      
        <text class="icon help"></text>
        <text class="txt">帮助中心</text>
      </navigator>
    </view>   
     <view class="item">
      <navigator url="/pages/ucenter/feedback/feedback" class="a">
        <text class="icon feedback"></text>
        <text class="txt">意见反馈</text>
      </navigator>
    </view> 
  </view>
  <!-- <view class="logout" bindtap="exitLogin">退出登录</view> -->
  <view class='footer-copyright'>
    <view class="footer-div">
      <!-- <image src="../../../static/images/logo.png" class="icon_copyright" />
      <text class="footer-zz">技术支持</text> -->
      <text class="footer-zz">版权归：贺宜伟所有</text>
      
    </view>
  </view>
</view>